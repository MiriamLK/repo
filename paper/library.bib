journal = {Reproductive BioMedicine Online},
keywords = {Ethics committees,FISH,Gender selection,Preimplantation genetic diagnosis},
number = {SUPPL. 2},
pages = {16--22},
pmid = {19891844},
publisher = {Reproductive Healthcare Ltd, Duck End Farm, Dry Drayton, Cambridge CB23 8DB, UK},
title = {{Preimplantation genetic diagnosis for gender selection in the USA}},
