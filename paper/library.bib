issn = {0002-9297},
journal = {American journal of human genetics},
number = {5},
pages = {1103--1111},
pmid = {6496474},
title = {{Paternal effects on the human sex ratio at birth: evidence from interracial crosses.}},
volume = {36},
year = {1984}
}
