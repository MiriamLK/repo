journal = {Review of Economic Studies},
keywords = {Adult equivalence scales,Collective model,Consumption inequality,Marital sorting},
number = {1},
pages = {328--355},
title = {{Consumption inequality and intra-household allocations}},
volume = {78},
year = {2011}
}
@article{Shenhav2016,
abstract = {This paper contributes new evidence on the role of the relative female to male wage as a monetary incentive (" bribe ") for women's family formation decisions, and spillovers of these decisions to the labor market. Exploiting task-based demand shifts as a shock to sex-specific wages, I show that a higher relative wage reduces marriage, particularly through delay of first marriage, and increases matching with higher-educated spouses. Further, women's increasing independence extends beyond marriage, shifting parent-ing and the household division of labor towards women. The estimates suggest that reductions in this incentive explain 20{\%} of the marriage decline from 1980 to 2010.},
author = {Shenhav, Na'ama},
