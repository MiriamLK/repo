@article{James2010a,
abstract = {The literature on occupational matching has traditionally assumed$\backslash$nthat an individual�s$\backslash$n$\backslash$nmatch in one occupation is completely uncorrelated with their match$\backslash$nin every other occupa-$\backslash$n$\backslash$ntion in the economy. In this paper I develop and estimate a model$\backslash$nthat relaxes this assumption.$\backslash$n$\backslash$nThe key feature of the model is that as an individual learns about$\backslash$ntheir occupation specific abil-$\backslash$n$\backslash$nity in one occupation, this will be broadly informative about their$\backslash$nability in other occupations.$\backslash$n$\backslash$nWorkers continually process their entire history of information which$\backslash$nthey use to determine$\backslash$n$\backslash$nwhen to change careers, as well as which new career to go to. Endogenizing$\backslash$ninformation in$\backslash$n$\backslash$nthis manner has been computationally prohibitive in the past. I extend$\backslash$nnew methods of solving$\backslash$n$\backslash$ndynamic discrete choice models with unobserved heterogeneity in a$\backslash$nunique way that facilitates$\backslash$n$\backslash$nthe estimation of an occupational choice model where individuals have$\backslash$ncorrelated unobserved$\backslash$n$\backslash$nheterogenous ability in every occupation. The model is estimated on$\backslash$nthe National Longitudinal$\backslash$n$\backslash$nSurvey of Youth 1997. The results suggest that occupational ability$\backslash$nshares a rich correlation$\backslash$n$\backslash$nstructure. While the gains from search are large, a counterfactual$\backslash$nshows that search frictions,$\backslash$n$\backslash$nincluding: risk aversion, destruction of occupation specific human$\backslash$ncapital, entry costs, and$\backslash$n$\backslash$nswitching costs, limit worker�s ability to find their comparative$\backslash$nadvantage.},
author = {James, Jon},
file = {:Users/Miriam/Box Sync/Bibliographies/Articles/James/Unknown/James - 2010 - Ability Matching and Occupational Choice.pdf:pdf},
title = {{Ability Matching and Occupational Choice}},
year = {2010}
}
@article{Goldin2014,
abstract = {The converging roles of men and women are among the grandest advances in society and the economy in the last century. These aspects of the grand gender convergence are figurative chapters in a history of gender roles. But what must the “last” chapter contain for there to be equality in the labor market? The answer may come as a surprise. The solution does not (necessarily) have to involve government intervention and it need not make men more responsible in the home (although that wouldn't hurt). But it must involve changes in the labor market, especially how jobs are structured and remunerated to enhance temporal flexibility. The gender gap in pay would be considerably reduced and might vanish altogether if firms did not have an incentive to disproportionately reward individuals who labored long hours and worked particular hours. Such change has taken off in various sectors, such as technology, science, and health, but is less apparent in the corporate, financial, and legal worlds. (JEL J3, J16, J22, J24, J31, J33, N3)},
author = {Goldin, Claudia},
doi = {10.1257/aer.104.4.1091},
file = {:Users/Miriam/Box Sync/Bibliographies/Articles/Goldin/American Economic Review/Goldin - 2014 - A Grand Gender Convergence Its Last Chapter.pdf:pdf},
issn = {0002-8282},
journal = {American Economic Review},
number = {4},
pages = {1091--1119},
title = {{A Grand Gender Convergence: Its Last Chapter}},
url = {http://pubs.aeaweb.org/doi/abs/10.1257/aer.104.4.1091},
volume = {104},
year = {2014}
}
@article{Chiappori2015a,
abstract = {Several theoretical contributions have argued that the returns to schooling within marriage play a crucial role for human capital investments. Our paper quantifies the evolution of these returns over the last decades. We consider a frictionless matching framework {\'{a}} la Becker-Shapley-Shubik, in which the gain generated by a match between two individuals is the sum of a systematic effect that only depends on the spouses' education classes and a match-specific term that we treat as random; following Choo and Siow (2006), we assume the latter component has an additively separable structure. We derive a complete, theoretical characterization of the model. We show that if the supermodularity of the surplus function is invariant over time and errors have extreme value distributions with time-invariant but education-dependent variances, the model is overidentified. We apply our method to US data on individuals born between 1943 and 1972. Our model fits the data very closely; moreover, we find that the deterministic part of the surplus is indeed supermodular and that, in line with theoretical predictions, the "marital college premium" has increased for women but not for men over the period.},
author = {Chiappori, Pierre-Andre and Salani{\'{e}}, Bernard and Weiss, Yoram},
file = {:Users/Miriam/Box Sync/Bibliographies/Articles/Chiappori, Salani{\'{e}}, Weiss/Working Paper/Chiappori, Salani{\'{e}}, Weiss - 2015 - Partner Choice and the Marital College Premium Analyzing Marital Patterns Over Several Decades(2).pdf:pdf},
journal = {Working Paper},
pages = {1--58},
title = {{Partner Choice and the Marital College Premium: Analyzing Marital Patterns Over Several Decades}},
year = {2015}
}
@article{Galichon2012,
abstract = {Which and how many attributes are relevant for the sorting of agents in a matching market? This paper addresses these questions by constructing indices of mutual attractiveness that aggregate information about agents' attributes. The first k indices for agents on each side of the market provide the best approximation of the matching surplus by a k-dimensional model. The methodology is applied on a unique Dutch household survey containing information about education, height, BMI, health, attitude towards risk and personality traits of spouses. Three important empirical conclusions are drawn. First, sorting in the marriage market is not unidimensional: individuals face important trade-offs between the attributes of their spouses which are not amenable to a single-dimensional index. Second, although education explains a quarter of a couple's observable surplus, personality traits explain another 20{\%}. Third, different personality traits matter differently for men and for women.},
author = {Galichon, Alfred and Dupuy, Arnaud},
doi = {10.2139/ssrn.2162812},
file = {:Users/Miriam/Box Sync/Bibliographies/Articles/Galichon, Dupuy/IZA Discussion Paper Series/Galichon, Dupuy - 2012 - Personality Traits and the Marriage Market.pdf:pdf},
