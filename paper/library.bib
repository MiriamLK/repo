journal = {American Economic Review},
keywords = {caroline hoxby,e24,inequality,j24,j31,j62,jel classifications,job tasks,joshua angrist,kerwin charles,luis garicano,maarten goos,o33,occu-,pational choice,polarization,service occupations,skill demand,technological change,we thank daron acemoglu},
