journal = {Economics of Education Review},
keywords = {Educational finance,Higher education,Hours of work,I21,J24,Part-time students},
pages = {38--50},
publisher = {Elsevier Ltd},
title = {{Working (and studying) day and night: Heterogeneous effects of working on the academic performance of full-time and part-time students}},
url = {http://dx.doi.org/10.1016/j.econedurev.2013.10.004},
volume = {38},
year = {2014}
}
@article{Ordine2011,
abstract = {This paper proposes a theoretical framework where within graduates wage inequality is related to overeducation/educational mismatch in the labor market. We show that wage inequality may arise because of inefficient self-selection into education in the presence of ability-complementary technological progress and asymmetric information on individuals' ability. In this setting, a crucial role is played by educational quality since it determines the signaling mechanisms in the labor market. We report some empirical evidence on the impact of mismatch, individual ability, family background, and university features on wages of Italian graduates and we suggest the importance of considering education quality as a policy instrument for reducing educational mismatch and wage inequality. {\textcopyright} 2011 Elsevier Ltd.},
