doi = {10.1016/j.jhealeco.2003.07.004},
file = {:Users/Miriam/Box Sync/Bibliographies/Articles/Currie, Hotz/Journal of Health Economics/Currie, Hotz - 2004 - Accidents will happen Unintentional childhood injuries and the effects of child care regulations.pdf:pdf},
issn = {01676296},
journal = {Journal of Health Economics},
keywords = {Accidents,Child care regulations,Childhood injuries},
