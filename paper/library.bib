journal = {Journal of Economic Behavior and Organization},
keywords = {Equalizing difference,Hedonic model,Heterogeneity,Matchingpattern,Two-sided matching,Wage differential,Worker productivity},
pages = {36--45},
title = {{Compensating wage differentials in stable job matching equilibrium}},
volume = {114},
