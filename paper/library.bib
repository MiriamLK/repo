journal = {Prenatal diagnosis},
keywords = {male sex selection,prenatal diagnosis,ultrasound},
number = {10},
pages = {980--984},
pmid = {16941719},
title = {{Prenatal diagnosis of the Cockayne syndrome: survey of 15 years experience.}},
volume = {26},
