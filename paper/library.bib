@article{Neuman1995,
abstract = {In this paper a distinction is made between human capital depreciation$\backslash$nrelated to a worker's aging and depreciation due to the obsolescence of$\backslash$nthe worker's education. Schooling-specific obsolescence of human capital$\backslash$nis incorporated in the Mincerian model of earnings, and it is shown how$\backslash$nthis obsolescence affects the worker's earnings profile. Using the$\backslash$nIsraeli 1983 Census we show that for `high-tech' oriented industries$\backslash$n(for which obsolescence is relatively important) obsolescence effects$\backslash$nare more significant than for `low-tech' oriented industries, and,$\backslash$nconsequently, the experience-earnings peak falls faster with increasing$\backslash$neducation in the former.},
